 local running = false

local Toggle = Tab:CreateToggle({
    Name = "Auto Buy Present V2 (Good).",
    CurrentValue = false,
    Flag = "Furniture Present",
    Callback = function(Value)
        running = Value
        if running then
            task.spawn(function()
                while running do
                    local VirtualInputManager = game:GetService("VirtualInputManager")
                    workspace:WaitForChild("PresentTable"):WaitForChild("Presents"):WaitForChild("Normal"):WaitForChild("ClickDetector"):WaitForChild("Detector"):FireServer()
                    task.wait(.086)
                    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.BackSlash, false, game)  -- key down
                    task.wait()
                    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.BackSlash, false, game)  -- key down
                    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Return, false, game)  -- key down
                    task.wait()
                    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.Return, false, game)  -- key down
                end
            end)
        end
    end,
})
local remote = game:GetService("ReplicatedStorage"):WaitForChild("PlayerChannel")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local running = false

local function sellAllWrappedItems()
    local backpack = LocalPlayer:WaitForChild("Backpack")
    local character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")

    for _, tool in pairs(backpack:GetChildren()) do
        if tool.Name == "Normal" and tool:FindFirstChild("Rarity") then
            if tool:FindFirstChild("PresentCamera") then
                tool.PresentCamera.Disabled = true
            end
            local id = tool:FindFirstChild("RewardAssetId") and tool.RewardAssetId.Value
            if id then
                humanoid:EquipTool(tool)
                remote:FireServer("PutUnwrappedItemInInventory", id)
                remote:FireServer("SellInventoryItem", id)
                tool:Destroy()
            end
        end
    end
end

local function autoSellLoop()
    while running do
        local backpack = LocalPlayer:WaitForChild("Backpack")
        local normalCount = 0
        for _, item in pairs(backpack:GetChildren()) do
            if item.Name == "Normal" then
                normalCount = normalCount + 1
            end
        end

        if normalCount > 5 then
            sellAllWrappedItems()
        end
        task.wait(1)
    end
end

local Toggle = Tab:CreateToggle({
    Name = "Auto Buy Present + Sell Instantly.",
    CurrentValue = false,
    Flag = "Furniture Present",
    Callback = function(Value)
        running = Value
        if running then
            task.spawn(function()
                local VirtualInputManager = game:GetService("VirtualInputManager")
                task.spawn(autoSellLoop)
                while running do
                    workspace:WaitForChild("PresentTable"):WaitForChild("Presents"):WaitForChild("Normal"):WaitForChild("ClickDetector"):WaitForChild("Detector"):FireServer()
                    task.wait(0.086)
                    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.BackSlash, false, game)
                    task.wait()
                    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.BackSlash, false, game)
                    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Return, false, game)
                    task.wait()
                    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.Return, false, game)
                end
            end)
        end
    end,
})

